$('#tagged_post_<%= @post.id %>').replaceWith('<%= j render(@post) %>');
$('#tagged_post_<%= @post.id %> #tagged_post_comments_<%= @post.id %> form div .comments_link').on('click', function() {
      return $(this).parent().parent().siblings('.comment_section').slideToggle();
});
$('#tagged_post_edit_form_<%= @post.id %>').hide();
$("#tagged_post_comment_<%= @post.id %>").hide();
$("#tagged_post_space_<%= @post.id %>").remove();
$("#tagged_post_<%= @post.id %>").hide().fadeIn(1000);